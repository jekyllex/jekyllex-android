<resources>
    <string name="app_name">JekyllEx</string>
    <string name="back_button">Go back</string>
    <string name="share_text">
        Hey, check out JekyllEx — The mobile IDE for Jekyll websites. It\'s free and open-source!
        https://jekyllex.xyz
    </string>

    <string name="process_notifications">Process notifications</string>
    <string name="process_notifications_id">process_notifications</string>
    <string name="process_notifications_desc">Native process service notifications</string>
    <string name="notification_text_title">Process bridge</string>
    <string name="notification_text_waiting">Waiting for a command…</string>
    <string name="notification_action_kill">Kill process</string>
    <string name="notification_action_stop">Stop service</string>

    <string name="installer_title">Installing bootstrap archive…</string>
    <string name="installer_subtitle">
        This may take a few minutes.
        You can close the app and come back later
        but please don\'t remove the app from history.
    </string>

    <string name="bootstrap_setting_title">Fix binaries</string>
    <string name="bootstrap_setting_summary">
        Attempt to re-install bundled binaries (ruby, coreutils, gems, etc.) if they go
        missing or corrupted.
    </string>

    <string name="trim_logs_title">Trim logs</string>
    <string name="trim_logs_summary">
        Try to trim command outputs to avoid excessive log sizes in the running session.
    </string>

    <string name="reduce_animations_title">Reduce animations</string>
    <string name="reduce_animations_summary">
        Skip rendering the file/folder details on the home page.
    </string>

    <string name="preview_port_title">Preview port</string>
    <string name="preview_port_summary">
        The localhost port number to load in the preview tab.
    </string>

    <string name="git_name_title">Name</string>
    <string name="git_name_summary">
        Global name to use for git operations.
    </string>

    <string name="git_email_title">Email</string>
    <string name="git_email_summary">
        Global email to use for git operations.
    </string>

    <string name="github_pat_title">GitHub access token</string>

    <string name="log_progress_title">Log progress</string>
    <string name="log_progress_summary">
        Append \"--progress\" to possible git commands.
    </string>

    <string name="local_gems_title">Prefer local gems</string>
    <string name="local_gems_summary">
        Prefer installed gems over latest versions when running \"bundle install\".
        Disabling will break installation of gems with native extensions.
        Hardcode the available gem version in the Gemfile if needed.
    </string>

    <string name="skip_bundler_title">Skip bundle install</string>
    <string name="skip_bundler_summary">
        Avoid \"bundle install\" for new projects. Uses the \"--skip-bundle\" flag.
    </string>

    <string name="prefix_bundler_title">Prefix bundler</string>
    <string name="prefix_bundler_summary">
        Prefix jekyll serve command with \"bundle exec\"
    </string>

    <string name="livereload_title">Live reload</string>
    <string name="livereload_summary">
        Enable livereload when jekyll serve command is executed. Auto appends
        \"-l\" to the \"jekyll serve\" command.
    </string>

    <string name="jekyll_env_title">Environment</string>
    <string name="jekyll_env_summary">
        Value for the JEKYLL_ENV environment variable
    </string>

    <string name="guess_urls_title">Guess destination URLs</string>
    <string name="guess_urls_summary">
        Try to guess the destination URL for files and load it the preview tab.
        Always load the home page otherwise.
    </string>

    <string name="jekyll_flags_title">Additional flags</string>
    <string name="jekyll_flags_summary">
        Space separated list of additional flags to pass to the \"jekyll serve\" command.
    </string>

    <string name="debounce_setting_title">Editor delay</string>
    <string name="debounce_setting_summary">
        Time to wait before saving changes to the file being edited (debounce delay).
    </string>

    <string name="theme_setting_title">Color theme</string>

    <string name="backup_setting_title">Backup</string>
    <string name="backup_setting_summary">
        Backup all projects to a zip file and store it at an external location.
    </string>
    <string name="restore_setting_title">Restore</string>
    <string name="restore_setting_summary">
        Choose a zip file to restore projects from. Wipes all existing data.
    </string>

    <string name="docs">Docs</string>
    <string name="report">Report</string>
    <string name="licenses">Licenses</string>
    <string name="privacy_policy">Privacy Policy</string>
    <string name="terms_and_conditions">Terms &amp; Conditions</string>
</resources>
